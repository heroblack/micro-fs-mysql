    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.3",
    "helmet": "^3.22.0",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "redis": "^3.0.2",
    "swagger-ui-express": "^4.1.4"
  },
  "devDependencies": {
    "chalk": "^4.0.0",
    "nodemon": "^2.0.3"
  }
